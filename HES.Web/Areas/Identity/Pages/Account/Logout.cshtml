@page
@model LogoutModel
@{
    ViewData["Title"] = "Log out";
}

<div class="row h-100 justify-content-center align-items-center">
    <div class="text-center">
        <h1 class="text-navyblue">@ViewData["Title"]</h1>
        <p class="text-gray">You have successfully logged out of the application.</p>
        <a asp-area="Identity" asp-page="/Account/Manage/Index">Go to login page</a>
    </div>
</div> 