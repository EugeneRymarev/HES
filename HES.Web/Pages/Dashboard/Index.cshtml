@page
@model HES.Web.Pages.Dashboard.IndexModel
@{
    ViewData["Title"] = "Dashboard";
    ViewData["ActivePage"] = MenuNavPages.Dashboard;
}
<partial name="_SuccessMessage" for="SuccessMessage" />
<partial name="_ErrorMessage" for="ErrorMessage" />

<h1 class="text-navyblue">Dashboard</h1>
<div id="content">
    <partial name="_DashboardContent" />
</div>

@section scripts{
    <script>
        $(document).ready(function () {
            // Breadcrumbs
            $('#breadcrumb').toggleClass('d-none');
            $('.breadcrumb').append('<li class="breadcrumb-item active">Dashboard</li>');
        });
    </script>
}