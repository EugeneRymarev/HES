@model HES.Web.Pages.Dashboard.IndexModel

<div class="col-12 col-md-6 col-xl-4">
    <div class="dashboard-card">
        <div class="dashboard-card-title">
            Workstations
        </div>
        <div class="dashboard-card-body">
            <div class="dashboard-card-body__logo">
                <img src="~/svg/dashboard/workstations.svg" />
            </div>
            <div class="dashboard-card-body__wrapper">
                <div class="dashboard-card__info">
                    <div class="dashboard-card__info-body font-weight-bold">@Model.RegisteredWorkstationsCount</div>
                    <div class="dashboard-card__info-title"><a class="dashboard-link" asp-page="/Workstations/Index">Registered</a></div>
                </div>
                <div class="dashboard-card__vertical-line"></div>
                <div class="dashboard-card__info">
                    <div class="dashboard-card__info-body">@Model.WorkstationsOnlineCount</div>
                    <div class="dashboard-card__info-title"><a class="dashboard-link" asp-page="./WorkstationsOnline">Online</a></div>
                </div>
            </div>
        </div>
    </div>
</div>