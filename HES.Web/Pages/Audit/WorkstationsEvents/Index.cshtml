@page
@model HES.Web.Pages.Audit.WorkstationsEvents.IndexModel

@{
    ViewData["Title"] = "Events";
    ViewData["ActivePage"] = MenuNavPages.Audit;
    ViewData["AuditPage"] = AuditNavPages.WorkstationsEvents;
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.WorkstationEvent[0].Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkstationEvent[0].EventId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkstationEvent[0].SeverityId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkstationEvent[0].Note)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkstationEvent[0].UserSession)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkstationEvent[0].Workstation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkstationEvent[0].Device)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkstationEvent[0].Employee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkstationEvent[0].Department)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkstationEvent[0].DeviceAccount)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.WorkstationEvent)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EventId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SeverityId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Note)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UserSession)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Workstation.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Device.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Employee.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Department.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DeviceAccount.Id)
                </td>
            </tr>
        }
    </tbody>
</table>
