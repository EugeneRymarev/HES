@if (Account == null)
{
    <LoadingText />
}
else
{
    <h5 class="text-gray mb-4 ">Set this account as a windows account?</h5>

    <div class="text-right">
        <button type="button" class="btn btn-secondary mr-1" @onclick="CloseAsync">Cancel</button>
        <button type="button" class="btn btn-primary" @onclick="SetAsWorkstationAccoountAsync">Set</button>
    </div>
}