@if (LicenseOrder == null)
{
    <LoadingText />
}
else
{
    <h5 class="text-gray mb-4 ">Are you sure you want to delete this order?</h5>
    <div class="text-right">
        <button type="button" class="btn btn-secondary" @onclick="CancelAsync">Cancel</button>
        <button type="button" class="btn btn-danger" @onclick="DeleteOrderAsync">Delete</button>
    </div>
}