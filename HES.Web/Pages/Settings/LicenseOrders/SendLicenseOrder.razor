@if (LicenseOrder == null)
{
    <LoadingText />
}
else
{
    <h5 class="text-gray mb-4 ">Confirm sending order.</h5>

    <div class="text-right">
        <button type="button" class="btn btn-secondary" @onclick="CancelAsync">Cancel</button>
        <button type="button" class="btn btn-primary" @onclick="SendOrderAsync">Send</button>
    </div>
}