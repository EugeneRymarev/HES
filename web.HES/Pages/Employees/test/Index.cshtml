@page
@model web.HES.Pages.Employees.test.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DeviceAccount[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceAccount[0].Urls)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceAccount[0].Apps)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceAccount[0].Login)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceAccount[0].Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceAccount[0].Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceAccount[0].LastSyncedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceAccount[0].CreatedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceAccount[0].PasswordUpdatedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceAccount[0].OtpUpdatedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceAccount[0].Deleted)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceAccount[0].Employee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceAccount[0].Device)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceAccount[0].SharedAccount)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.DeviceAccount) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Urls)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apps)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Login)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastSyncedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PasswordUpdatedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OtpUpdatedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Deleted)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Employee.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Device.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SharedAccount.Id)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
